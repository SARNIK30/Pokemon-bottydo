run = "python3 run_polling_bot.py"
language = "python3"
modules = ["nodejs-20", "python-3.11"]

[env]
PATH = "/home/runner/workspace/.pythonlibs/bin:/home/runner/.nix-profile/bin:/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin"

[[ports]]
localPort = 5000
externalPort = 5000

[[ports]]
localPort = 8080
externalPort = 80

[deployment]
run = "python3 run_polling_bot.py"
deploymentTarget = "cloudrun"

[nix]
channel = "stable-22_11"
